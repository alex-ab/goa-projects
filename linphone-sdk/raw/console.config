<config verbose="yes">

	<parent-provides>
		<service name="ROM"/>
		<service name="LOG"/>
		<service name="RM"/>
		<service name="CPU"/>
		<service name="PD"/>
		<service name="Audio_out"/>
		<service name="Audio_in"/>
		<service name="Gui"/>
		<service name="Timer"/>
		<service name="Nic"/>
	</parent-provides>

	<default-route>
		<any-service> <parent/> <any-child/> </any-service>
	</default-route>

	<default caps="100"/>

	<start name="terminal" caps="110">
		<resource name="RAM" quantum="10M"/>
		<provides> <service name="Terminal"/> </provides>
		<route>
			<service name="ROM" label="config">
				<parent label="terminal.config"/> </service>
			<any-service> <parent/> </any-service>
		</route>
	</start>

	<start name="linphonec" caps="800">
		<resource name="RAM" quantum="128M"/>
		<route>
			<service name="ROM" label="liblinphone.lib.so.10">   <parent label="liblinphone.lib.so"/>   </service>
			<service name="ROM" label="libortp.lib.so.15">       <parent label="libortp.lib.so"/>       </service>
			<service name="ROM" label="libbctoolbox.lib.so.1">   <parent label="libbctoolbox.lib.so"/>  </service>
			<service name="ROM" label="libmbedtls.lib.so.19">    <parent label="libmbedtls.lib.so"/>    </service>
			<service name="ROM" label="libmbedx509.lib.so.5">    <parent label="libmbedx509.lib.so"/>   </service>
			<service name="ROM" label="libmbedcrypto.lib.so.14"> <parent label="libmbedcrypto.lib.so"/> </service>
			<service name="ROM" label="libdecaf.lib.so.0">       <parent label="libdecaf.lib.so"/>      </service>
			<service name="ROM" label="libsrtp2.lib.so.5.3.0">   <parent label="libsrtp2.lib.so"/>      </service>
			<service name="ROM" label="libopus.lib.so.0">        <parent label="libopus.lib.so"/>       </service>
			<service name="ROM" label="libbzrtp.lib.so.0">       <parent label="libbzrtp.lib.so"/>      </service>
			<service name="ROM" label="libbelle-sip.lib.so.1">   <parent label="libbelle-sip.lib.so"/>  </service>
			<service name="ROM" label="libbelr.lib.so.1">        <parent label="libbelr.lib.so"/>       </service>
			<service name="ROM" label="libz.lib.so.1">           <parent label="libz.lib.so"/>          </service>
			<service name="ROM" label="libxml2.lib.so.2">        <parent label="libxml2.lib.so"/>       </service>
			<service name="Terminal"> <child name="terminal"/> </service>
			<any-service> <parent/> </any-service>
		</route>
		<config ld_verbose="true">

			<env key="HOME" value="/"/>

			<arg value="linphonec"/>
			<arg value="-b"/>
			<arg value="factory.settings"/>
			<arg value="-l"/>
			<arg value="3"/>

			<libc stdin="/dev/terminal" stdout="/dev/terminal" stderr="/dev/log"
			      rtc="/dev/rtc" socket="/socket" pipe="/pipe" rng="/dev/random"/>

			<vfs>
				<dir name="dev">
						<log/><null/><inline name="rtc">2000-01-01 00:00</inline>
						<jitterentropy name="random"/>
						<oss name="dsp"/>
						<terminal/>
				</dir>
				<dir name="pipe"> <pipe/> </dir>
				<dir name="socket"> <lxip dhcp="yes"/> </dir>
				<dir name=".local">
					<dir name="share">
						<dir name="linphone">
							<ram/>
						</dir>
					</dir>
				</dir>
				<rom name="factory.settings"/>
				<rom name="hello8000.wav"/>
				<rom name="hello44100.wav"/>
				<ram/>
			</vfs>
		</config>
	</start>

</config>
